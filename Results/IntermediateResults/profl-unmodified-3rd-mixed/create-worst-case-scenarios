#!/bin/bash

cd worst-case
# cp -r /media/disk2/sam/profl-unmodified-5th/best-case/ProFL-* ./
rm -rf aggregat*
./create-aggregate
cd ../

priority=100

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-KaliA -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-GenProgA -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-RSRepair -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-Arja -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-ACS -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-AvatarFixer -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-FixMiner -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-kParFixer -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-TBarFixer -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-Dynamoth -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-Simfix -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-jKali -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-jGenProg -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-jMutRepair -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-Cardumen -type f -name a*.profl)
python3 reorder-unmodified-mix.py ${priority} $(find worst-case/ProFL-PraPR -type f -name a*.profl)

python3 reorder-unmodified-mix.py ${priority} $(find worst-case/aggregation -type f -name a*.profl)
#################################################################

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-KaliA -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-GenProgA -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-RSRepair -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Arja -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-ACS -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-AvatarFixer -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-FixMiner -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-kParFixer -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-TBarFixer -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Dynamoth -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Simfix -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jKali -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jGenProg -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jMutRepair -type f -name *.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Cardumen -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-PraPR -type f -name *.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/aggregation -type f -name *.profl)

cd worst-case
./run
cd ../

