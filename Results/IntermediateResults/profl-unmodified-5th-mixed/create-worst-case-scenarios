#!/bin/bash

#cd worst-case
#rm -rf aggregat*
#./create-aggregate
#cd ../

priority=1

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-KaliA -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-GenProgA -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-RSRepair -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Arja -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-ACS -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-AvatarFixer -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-FixMiner -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-kParFixer -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-TBarFixer -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Nopol -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Dynamoth -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Simfix -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jKali -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jGenProg -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-jMutRepair -type f -name *SusInfo.profl)
python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-Cardumen -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/ProFL-PraPR -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find worst-case/aggregation -type f -name *SusInfo.profl)

python3 py-scripts/readjust-profl-worse-case-rank.py ${priority} $(find common -type f -name *SusInfo.profl)

cd worst-case
./run
cd ../
