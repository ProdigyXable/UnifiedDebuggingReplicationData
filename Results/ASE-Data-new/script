#!/bin/bash

p=(Chart Math Lang Time Closure Mockito)

for i in $(seq 133)
do
	for p in ${p[@]}
	do
		
		mv $1/$p-$i/proflvariant-fullextended $1/$p-$i/proflvariant-full-extended
		mv $1/$p-$i/proflvariant-fullstandard $1/$p-$i/proflvariant-full-standard
		mv $1/$p-$i/proflvariant-partialextended $1/$p-$i/proflvariant-partial-extended
		mv $1/$p-$i/proflvariant-partialstandard $1/$p-$i/proflvariant-partial-standard
		
		mv $1/$p-$i/proflvariant-statementlevel-fullextended $1/$p-$i/proflvariant-statementlevel-full-extended
		mv $1/$p-$i/proflvariant-statementlevel-fullstandard $1/$p-$i/proflvariant-statementlevel-full-standard
		mv $1/$p-$i/proflvariant-statementlevel-partialextended $1/$p-$i/proflvariant-statementlevel-partial-extended
		mv $1/$p-$i/proflvariant-statementlevel-partialstandard $1/$p-$i/proflvariant-statementlevel-partial-standard	
	
	done
done
